            <kind>1</kind>
            <string>// don't go into walls

// horizontal snap onto solid
// if player is going to move into a solid in the x direction...
if (dx!=0 &amp;&amp; !place_free(self.x+dx,self.y)) {

    // clip onto the solid in question
    // NOTE: 0 means right and 180 means left
    // NOTE: -dx is because the left is the negative x direction
    if (dx&gt;0) move_contact_solid(0,dx);
    if (dx&lt;0) move_contact_solid(180,-dx);
    
    // and stop moving sanpping
    dx=0;

}

// vertical snap onto solid
// if player is going to move into a solid in the y direction...
if (dy!=0 &amp;&amp; !place_free(self.x,self.y+dy)) {

    // clip onto the solid in question
    // NOTE: 90 means up and 270 means down
    // NOTE: -dy is because up is the negative y direction
    if (dy&gt;0) move_contact_solid(270,dy);
    if (dy&lt;0) move_contact_solid(90,-dy);
    
    // and stop moving after sanpping
    dy=0;

}

// move after snapping to walls
self.x+=dx
self.y+=dy
</string>
