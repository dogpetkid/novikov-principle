        <arguments>
          <argument>
            <kind>1</kind>
            <string>// jump back to clone

// when space is pressed...
if (keyboard_check_pressed(vk_space) &amp;&amp; clone_running) {

    // with the clone, make it set the player's y value
    // (0 means the 0th isntance, similar to the 0th element of an array)
    // other = self means set the player = clone
    with (instance_find(obj_clone,0)) other.x = self.x;
    with (instance_find(obj_clone,0)) other.y = self.y;
    
    event_user(2); // FUNCTION: delete the clonne path
    
    // stop the player no matter what to not create a new clone on this step
    dx=0;
    dy=0;
}
</string>
