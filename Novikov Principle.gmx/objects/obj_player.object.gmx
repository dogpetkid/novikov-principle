          <argument>
            <kind>1</kind>
            <string>// handle the clone spawning

// prepare a clone spawn if it is not spawned
if (!clone_spawning &amp;&amp; !clone_running) { 

    // if the player is moving then the clone will spawn in
    if (dx!=0 || dy!=0) {
    
        clone_spawning = true;
        // this alarm will actually spawn in the clone
        alarm[0] = 4 * room_speed; // 4 second
        
        // POSSIBLE: add a particle spawner where the clone will be to warn the player
        
        
        global.clonePath = path_add();
        // make the path stright, so point to point instead of curvy
        path_set_kind(global.clonePath,0);
        
        // create a starting point where player is (100 means 100% path speed)
        path_add_point(global.clonePath, self.x, self.y, 100);
        // sqrt(sqr(dx)+sqr(dy)) is the speed in combined x and y
        
        // record spawn coordinates
        cloneSpawnx=self.x;
        cloneSpawny=self.y;
    
    }

}

// at changes in motion...
// ALWAYS FALSE TO REMOVE ODD ACTION
if (false){//prevdx!=dx || prevdy!=dy) {
    // add a new point to go to, thus recording the positions of the player in a path
    path_add_point(global.clonePath, self.x, self.y, 100);
}

// save dx and dy for next iteration of step
prevdx=dx
prevdy=dy
</string>
