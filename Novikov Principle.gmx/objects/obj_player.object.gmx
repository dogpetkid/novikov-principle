dy = 0;

// register move up
if (keyboard_check(vk_up) || keyboard_check(ord('W'))) {
    dy+=-defspeed;
}

// register move down
if (keyboard_check(vk_down) || keyboard_check(ord('S'))) {
    dy+= defspeed;
}

// register move left
if (keyboard_check(vk_left) || keyboard_check(ord('A'))) {
    dx+=-defspeed;
}

// register move down
if (keyboard_check(vk_right) || keyboard_check(ord('D'))) {
    dx+= defspeed;
}
