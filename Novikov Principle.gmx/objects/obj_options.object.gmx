            <kind>1</kind>
            <string>// check for clicking inside boxes

// scp_button_pressed(x1,y1,x2,y2);

// back
if(scp_button_pressed(x1back,y1back,x2back,y2back) || 
   keyboard_check_pressed(vk_escape)) {
    
    room_goto(rm_main_menu);

}

// Delete save data
if(scp_button_pressed(x1del,y1del,x2del,y2del)) {

    if (show_question("Are you sure you want to delete your save data?")) {
        ini_open("save.ini");
        // levels stores all the saved progress data
        ini_section_delete("levels");
        ini_close();
        show_message("Data deleted.");
    } else {
        show_message("Data preserved.");
    }
    
    // Restore fullscreen if broken
    scp_fix_fullscreen();
    

}

// Fullscreen
if(scp_button_pressed(x1ful,y1ful,x2ful,y2ful)) {
    
    // toggle window setting
    window_set_fullscreen(!window_get_fullscreen());
    ini_open("save.ini");
    // save setting
    ini_write_real("options","fullscreen",window_get_fullscreen());
    ini_close();

}
</string>
