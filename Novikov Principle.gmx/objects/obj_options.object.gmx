            <string>// prepare clickboxes

draw_set_font(fnt_menu);

// Back
tback = "back";
x1back = 0.1*room_width;
y1back = 0.85*room_height;
x2back = x1back + string_width(tback);
y2back = y1back + string_height(tback);

// Remove save data
tdel = "Delete save data";
x1del = 1/2 * hanchor - string_width(tdel)/2; // shift over to the (upper) left
y1del = 1/2 * vanchor; // shift over to the upper (left)
x2del = x1del + string_width(tdel);
y2del = y1del + string_height(tdel);

// Fullscreen
tful = "Fullscreen";
x1ful = 1/2 * hanchor - string_width(tful)/2;
y1ful = y1del + 1.5*string_height(tdel); // shift under Remove save data
x2ful = x1ful + string_width(tful);
y2ful = y1ful + string_height(tful);
</string>
