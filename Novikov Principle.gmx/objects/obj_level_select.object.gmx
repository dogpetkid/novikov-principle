if(scp_button_pressed(x1b2,y1b2,x2b2,y2b2)) {

    if (lb2 == ini_read_real("levels","b2",0))
    room_goto(lb2);

}

if(scp_button_pressed(x1trp,y1trp,x2trp,y2trp)) {

    if (ltrp == ini_read_real("levels","trp",0))
    room_goto(ltrp);
    
}

