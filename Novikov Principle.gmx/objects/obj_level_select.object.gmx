    if (lbut == ini_read_real("levels","but",0))
    room_goto(lbut);

}

// Pixel Perfect
if(scp_button_pressed(x1pp,y1pp,x2pp,y2pp)) {

    if (lpp == ini_read_real("levels","pp",0))
    room_goto(lpp);

}

// Extended (pixel) Perfect
if(scp_button_pressed(x1ep,y1ep,x2ep,y2ep)) {
    
