   keyboard_check_pressed(vk_escape)) {
    
    room_goto(rm_main_menu);

}

// Tutorial
if(scp_button_pressed(x1tut,y1tut,x2tut,y2tut)) {

    
    room_goto(ltut);

}



// make sure the player has been to that section for all sections below

// Button
